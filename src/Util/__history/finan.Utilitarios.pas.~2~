unit finan.Utilitarios;

interface
type
  TUtilitarios = class
    class function GetId : String;
  end;

implementation

uses
  System.SysUtils;

{ TUtilitarios }

class function TUtilitarios.GetId: String;
begin
  Result := TGUID.NewGuid.toString;
  Result := StringReplace(Result, '{', '', [rFReplaceAll]);
  Result := StringReplace(Result, '}', '', [rFReplaceAll]);
end;

end.
